    <string name="download_in_process">Memuat turun</string>
    <string name="download_complete">Telah dimuat turun</string>

    <string name="retry_after_network_available">Sila cuba semula apabila sambungan rangkaian tersedia</string>

    <string name="editmode_menu_share">Kongsi</string>
    <string name="editmode_menu_delete">Padam</string>
    <string name="all_downloads">Semua muat turun</string>
    <string name="app_downloads"><xliff:g id="pkgname">%s</xliff:g> muat turun</string>

