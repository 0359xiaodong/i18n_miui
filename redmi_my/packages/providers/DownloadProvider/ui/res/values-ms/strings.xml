    <string name="menu_enter_edit_mode">Editor</string>
    <string name="delete_downloads_records">Padam muat turun</string>

    <string name="pause_download">Henti seketika</string>
    <string name="resume_download">Sambung semula</string>
    <string name="restart_download">Mula semula</string>
    <string name="open_download">Buka</string>

    <string name="paused_by_app">Henti seketika secara manual</string>
    <string name="paused_queued_for_wifi">Menunggu sambungan Wi-Fi</string>
    <string name="paused_waiting_for_network">Menunggu sambungan rangkaian</string>
    <string name="paused_waiting_to_retry">Menunggu untuk mencuba semula</string>
    <string name="paused_unknown">Dihentikan seketika untuk alasan yang tidak diketahui</string>

    <string name="confirm_delete_downloads_message">%d muat turun yang dipilih akan dibuang!</string>
    <string name="confirm_delete_the_download_item_message">Sahkan pembuangan muat turun!</string>

    <string name="delete_the_downloaded_file">Padamkan fail yang telah dimuat turun.</string>
    <string name="downloads_operating">Sedang beroperasi...</string>

    <string name="select_operation_items">Sila pilih item operasi</string>
    <string name="preferences_title">Tetapan</string>
    <string name="pref_download_settings_title">Penggunaan data</string>
    <string name="pref_title_enable_max_bytes_popup">Pemberitahuan muat turun data</string>
    <string name="pref_summary_enable_max_bytes_popup">Di bawah rangkaian pengendali, pemberitahuan timbul jika saiz fail mencapai had</string>
    <string name="pref_summary_max_bytes_limit_format">Beratur untuk rangkaian Wi-Fi jika saiz fail mencapai <xliff:g id="number">%s</xliff:g></string>
