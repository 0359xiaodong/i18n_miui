<resources xmlns:android="http://schemas.android.com/apk/res/android"
           xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Label for this app's shared user ID (and thus for all other
         apps sharing the uid). -->
    <string name="sharedUserLabel">Aplikasi Teras Android</string>

    <!-- This is the label for the application that stores contacts data -->
    <string name="app_label">Simpanan Kenalan</string>

    <!-- What to show in messaging that refers to this provider, e.g. AccountSyncSettings -->
    <string name="provider_label">Kenalan</string>

    <!-- [CHAR LIMIT=NONE] Boot message while upgrading contacts. -->
    <string name="upgrade_msg">Pangkalan data Kenalan sedang dinaik taraf.</string>

    <!-- Ticker for the notification shown when updating contacts fails because of memory shortage -->
    <string name="upgrade_out_of_memory_notification_ticker">Penaik-tarafan Kenalan memerlukan lebih memori.</string>

    <!-- Title for the notification shown when updating contacts fails because of memory shortage -->
    <string name="upgrade_out_of_memory_notification_title">Simpanan kenalan sedang dinaik taraf</string>

    <!-- Text for the notification shown when updating contacts fails because of memory shortage -->
    <string name="upgrade_out_of_memory_notification_text">Pilih untuk melengkapkan proses naik taraf.</string>

    <!-- The name of the default contact directory -->
    <string name="default_directory">Kenalan</string>

    <!-- The name of the invisible local contact directory -->
    <string name="local_invisible_directory">Lain-lain</string>

    <!-- Title of an application permission, listed so the user can choose whether they want to
     allow the application to do this.  [CHAR LIMIT=NONE] -->
    <string name="read_write_all_voicemail_label">Akses semua mel suara</string>
    <!-- Description of an application permission, listed so the user can choose whether
         they want to allow the application to do this. [CHAR LIMIT=NONE] -->
    <string name="read_write_all_voicemail_description">Membenarkan aplikasi untuk menyimpan dan mendapatkan 
    kembali semua mel suara yang boleh diakses peranti ini.</string>

    <!-- The prefix string before the number used for the display name for the voicemail table.
         Because of the way this is combined in SQL we can't allow a generic format string.
         The resulting string will be this string with the number appended to the end.
         Note that the trailing space is important, and that to achieve it we have to wrap the
         string in double quotes. -->
    <string name="voicemail_from_column">"Mel suara daripada "</string>

    <!-- Debug tool - title of the dialog which copies the contact database into the external storage. [CHAR LIMIT=NONE] -->
    <string name="debug_dump_title">Salin pangkalan data kenalan</string>

    <!-- Debug tool - message shown to the user on the dialog which sends a copy of  the contact database via email or other apps. [CHAR LIMIT=NONE] -->
    <string name="debug_dump_database_message">Anda akan 1) membuat salinan pangkalan data yang mengandungi maklumat berkenaan semua kenalan dan semua log panggilan kepada Simpanan Dalaman, dan 2) e-melkannya. Ingat untuk membuangkan salinan tersebut sebaik sahaja anda berjaya menyalinkan ia diluar peranti atau e-mel telah diterima.</string>

    <!-- Debug tool - dialog button- delete file now [CHAR LIMIT=NONE] -->
    <string name="debug_dump_delete_button">Buang sekarang</string>

    <!-- Debug tool - dialog button - start copying [CHAR LIMIT=NONE] -->
    <string name="debug_dump_start_button">Mula</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_dump_email_sender_picker">Pilih program untuk menghantar fail anda</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_dump_email_subject">Contacts DB dilampirkan</string>

    <!-- Debug tool - email body [CHAR LIMIT=NONE] -->
    <string name="debug_dump_email_body">Dilampirkan adalah pangkalan data kenalan dengan semua maklumat kenalan saya. Kendalikan dengan baik.</string>

    <string name="third_party_delete_contact_info">\"%s\" sedang membuangkan kenalan Xiaomi anda, sila gunakan Kenalan sistem untuk keselamatan tambahan kenalan anda.</string>
</resources>
