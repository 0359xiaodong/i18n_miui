<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- The name of the application that appears in the launcher [CHAR LIMIT=15] -->
    <string name="app_label">Downloads</string>
    <!-- The title that appears at the top of the activity listing downloads [CHAR LIMIT=25] -->
    <string name="download_title">Downloads</string>
    <!-- The title that appears at the top of the activity listing downloads sorted by date [CHAR LIMIT=40] -->
    <string name="download_title_sorted_by_date">Downloads - Susun mengikut tarikh</string>
    <!-- The title that appears at the top of the activity listing downloads sorted by size [CHAR LIMIT=40] -->
    <string name="download_title_sorted_by_size">Downloads - Susun mengikut saiz</string>

    <!-- Appears in lieu of the list of downloads if there are no downloads to view
         [CHAR LIMIT=200] -->
    <string name="no_downloads">Tiada downloads</string>

    <!-- Default title for an item in the download list for which no title was provided by the app.
         [CHAR LIMIT=20] -->
    <string name="missing_title">&lt;Tidak diketahui&gt;</string>

    <!-- Button to display sort-by-size option [CHAR LIMIT=25] -->
    <string name="button_sort_by_size">Susun mengikut saiz</string>
    <!-- Button to display sort-by-date option [CHAR LIMIT=25] -->
    <string name="button_sort_by_date">Susun mengikut tarikh</string>

    <!-- Status messages -->

    <!-- Status indicating that the download has been queued to start in the future.  Appears for an
         individual item in the download list.  [CHAR LIMIT=11] -->
    <string name="download_queued">Digilirkan</string>
    <!-- Status indicating that the system is currently downloading the file.  Appears for an
         individual item in the download list.  [CHAR LIMIT=11] -->
    <string name="download_running">Downloading</string>
    <!-- Status indicating that the download has completed successfully. Appears for an individual
         item in the download list.  [CHAR LIMIT=11] -->
    <string name="download_success">Selesai</string>
    <!-- Status indicating that the download has ended without completing successfully.  Appears for
         an individual item in the download list.  [CHAR LIMIT=11] -->
    <string name="download_error">Gagal</string>
    <!-- Status indicating that the download has ended due to insufficient space.  Appears for
         an individual item in the download list.  [CHAR LIMIT=11] -->
    <string name="download_error_insufficient_space">Ruang tidak mencukupi</string>

    <!-- Dialog/toast messages -->

    <!-- Title of dialog that is shown when the user clicks a download for which no file is
         available, either because the download hasn't started or because the download failed
         [CHAR LIMIT=25] -->
    <string name="dialog_title_not_available">Download Gagal</string>
    <!-- Text for dialog when user clicks on a download that failed [CHAR LIMIT=200] -->
    <string name="dialog_failed_body">Adakah anda ingin untuk mencuba semula download fail
    kemudian atau memadamnya dari giliran?</string>
    <!-- Title for dialog when user clicks on a download that has not yet begun, but will be started
         in the future. [CHAR LIMIT=25] -->
    <string name="dialog_title_queued_body">Fail tidak tersedia</string>
    <!-- Text for dialog when user clicks on a download that has not yet begun, but will be started
         in the future. [CHAR LIMIT=200] -->
    <string name="dialog_queued_body">Fail ini digilir untuk download kemudian.</string>
    <!-- Title for dialog when user clicks on a completed download but the file is missing-->
    <string name="dialog_file_missing_title">Gagal untuk membuka fail</string>
    <!-- Text for dialog when user clicks on a completed download but the file is missing
         [CHAR LIMIT=200] -->
    <string name="dialog_file_missing_body">Fail yang telah download tidak dijumpai.</string>
    <!-- Text for dialog when user clicks on a download that failed due to insufficient space on
         external storage [CHAR LIMIT=200] -->
    <string name="dialog_insufficient_space_on_external">Download tidak dapat dihabiskan. Tiada ruang yang 
    mencukupi pada storan luaran anda.</string>
    <!-- Text for dialog when user clicks on a download that failed due to insufficient space on
         the internal download cache [CHAR LIMIT=200] -->
    <string name="dialog_insufficient_space_on_cache">Download tidak dapat dihabiskan. Tiada ruang yang 
    mencukupi pada storan dalaman anda.</string>
    <!-- Text for dialog when user clicks on a download that failed because it was interrupted and
         the server doesn't support resuming downloads [CHAR LIMIT=200] -->
    <string name="dialog_cannot_resume">Download turun terganggu, tidak boleh disambung semula.</string>
    <!-- Text for dialog when user clicks on a download that failed because the requested
         destination file already exists [CHAR LIMIT=200] -->
    <string name="dialog_file_already_exists">Tidak dapat download, destinasi fail tersebut telah wujud.
    </string>
    <!-- Text for dialog when user clicks on a download that failed because it was requested to go
         on the external media, which is not mounted [CHAR LIMIT=200] -->
    <string name="dialog_media_not_found">Tidak dapat download. Media luaran tidak tersedia.
    </string>
    <!-- Text for a toast appearing when a user clicks on a completed download, informing the user
         that there is no application on the device that can open the file that was downloaded
         [CHAR LIMIT=200] -->
    <string name="download_no_application_title">Fail tidak dapat dibuka</string>

    <!-- Buttons -->

    <!-- Text for button to remove the entry for a download that has not yet begun or that has
         failed [CHAR LIMIT=25] -->
    <string name="remove_download">Buang</string>
    <!-- Text for button to delete a download that has completed, or to delete multiple download
         entries [CHAR LIMIT=25] -->
    <string name="delete_download">Padam</string>
    <!-- Text for button to keep a download that has not yet begun [CHAR LIMIT=25] -->
    <string name="keep_queued_download">Keep</string>
    <!-- Text for button to cancel a download that is currently in progress [CHAR LIMIT=25] -->
    <string name="cancel_running_download">Simpan</string>
    <!-- Text for button appearing in a dialog to restart a download, either one that failed or one
         for which the downloaded file is now missing [CHAR LIMIT=25] -->
    <string name="retry_download">Cuba semula</string>
    <!-- Text for button appearing in the pop-up selection menu to deselect all currently selected
    downloads in the download list [CHAR LIMIT=25] -->
    <string name="deselect_all">Jangan pilih semua</string>
    <!-- Text for menu to select all downloads in the download list [CHAR LIMIT=25] -->
    <string name="select_all">Pilih Semua</string>
    <!-- number of downloads selected [CHAR LIMIT=50] -->
    <string name="selected_count">Pilih <xliff:g id="number" example="3">%1$d</xliff:g> dari <xliff:g id="total" example="15">%2$d</xliff:g></string>
    <!-- title shown when displaying list of apps to choose from when share is clicked on
        [CHAR LIMIT = 20] -->
    <string name="download_share_dialog">Kongsi melalui</string>

    <string name="download_when_network_connected">"Download menggunakan rangkaian Mudah alih"</string>

    <string name="download_paused">"Download diberhentikan"</string>
    <string name="download_pending">"Download sedang menunggu"</string>
    <string name="download_info_start_deleted_tasks">Padam downloads</string>
    <string name="download_info_count_deleted_tasks">%s downloads disingkirkan</string>

    <string name="menu_preferences">Aturan</string>
    <string name="menu_enter_edit_mode">Editor</string>
    <string name="delete_downloads_records">Padamkan downloads</string>

    <string name="pause_download">Pause</string>
    <string name="resume_download">Sambung</string>
    <string name="restart_download">Restart</string>
    <string name="open_download">Buka</string>

    <string name="paused_by_app">Berhenti seketika secara manual</string>
    <string name="paused_queued_for_wifi">Menunggu sambungan Wi-Fi</string>
    <string name="paused_waiting_for_network">Menunggu sambungan rangkaian</string>
    <string name="paused_waiting_to_retry">Menunggu untuk mencuba semula</string>
    <string name="paused_unknown">Ralat tidak diketahui</string>

    <string name="confirm_delete_downloads_message">%d downloads yang dipilih akan disingkirkan!</string>
    <string name="confirm_delete_the_download_item_message">Sila sahkan penyingkiran download!</string>

    <string name="delete_the_downloaded_file">Padamkan fail-fail yang telah download.</string>
    <string name="downloads_operating">Sedang beroperasi...</string>

    <string name="select_operation_items">Sila pilih item-item</string>
    <string name="preferences_title">Aturan</string>
    <string name="pref_download_settings_title">Pemberitahuan penggunaan data</string>
    <string name="pref_title_enable_max_bytes_popup">Pemberitahuan data download</string>
    <string name="pref_summary_enable_max_bytes_popup">Di bawah rangkaian pengendali, pemberitahuan timbul jika saiz fail melebihi had</string>
    <string name="pref_summary_max_bytes_limit_format">Pemberitahuan timbul jika saiz fail melebihi <xliff:g id="number">%s</xliff:g></string>
    <string name="pref_summary_any_bytes_popup">Sentiasa bergilir apabila menggunakan rangkaian Wi-Fi</string>
    <string name="pref_summary_no_limit_popup">Saiz download adalah tidak terhad</string>

    <string name="download_in_process">Sedang download</string>
    <string name="download_complete">Telah Download</string>

    <string name="retry_after_network_available">Sila cuba semula apabila sambungan rangkaian boleh didapati</string>

    <string name="editmode_menu_share">Kongsi</string>
    <string name="editmode_menu_delete">Padam</string>
    <string name="all_downloads">Semua downloads</string>
    <string name="app_downloads"><xliff:g id="pkgname">%s</xliff:g> downloads</string>

    <!-- display xunlei brand -->
    <string name="xunlei_brand">Powered by Xunlei</string>
    <!-- xunlei brand url -->
    <string name="xunlei_brand_url">http://mobile.xunlei.com</string>
