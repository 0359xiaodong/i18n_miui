    <string name="install_failed_verify_failed">Paketnya tidak melewati verfikasi dan tidak dapat
        dipasang.</string>
    <!-- Reason displayed when installation fails because the package could not be verified
        before the internal system timer expired. [CHAR LIMIT=100] -->
    <string name="install_failed_verify_timeout">Terjadi timeout ketika mencoba memverifikasi paket
        ini. Coba untuk pasang lagi nanti.</string>
    <!-- Message presented when an application could not be installed on the tablet for some reason. [CHAR LIMIT=100] -->
    <string name="install_failed_msg" product="tablet"><xliff:g id="app_name">%1$s</xliff:g> tidak dapa dipasang di tablet ini.</string>
