<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">Pesquisar</string>

    <!-- Search widget -->
    <string name="search_widget">Pesquisar</string>

    <!-- Text in the dialog for confirming whether to remove an item from the search
         history. [CHAR_LIMIT=NONE] -->
    <string name="remove_from_history">Excluir do histórico de pesquisa</string>

    <!-- Toast text shown when the app fails to remove an item from
         the search history. [CHAR_LIMIT=60] -->
    <string name="remove_from_history_failed">"Falha ao excluir item do histórico"</string>

    <!-- Source selector -->
    <string name="corpus_selection_heading">Pesquisar</string>
    <string name="corpus_label_global">Tudo</string>

    <!-- Name of the combined Web source shown in QSB -->
    <string name="corpus_label_web">Web</string>
    <!-- Description of the combined Web source shown in QSB -->
    <string name="corpus_description_web">Pesquisa web, favoritos e históricos de navegação</string>

    <!-- Name of the Apps source shown in QSB -->
    <string name="corpus_label_apps">Aplicativos</string>
    <!-- Settings description for the Apps source shown in QSB -->
    <string name="corpus_description_apps">Nome dos aplicativos instalados</string>
    <!-- Search hint for the Apps source shown in QSB -->
    <string name="corpus_hint_apps">Pesquisar aplicativos</string>

    <!-- Search settings stuff -->

    <!-- Search settings menu item -->
    <string name="menu_settings">Configurações de pesquisa</string>

    <!-- Search settings title -->
    <string name="search_settings">Configurações de pesquisa</string>

    <!-- Title for Google Account category of search settings.
         [CHAR LIMIT=20] -->
    <string name="google_settings_category_title">Pesquisa Google</string>

    <!-- Title for 'device' category of search settings 
        [CHAR LIMIT=20] -->
    <string name="device_settings_category_title">Limpar atalhos</string>

    <!-- Title for 'system search' category of search settings -->
    <string name="system_search_category_title">Telefone</string>

    <!-- Title and summary for 'search sources' link for system search -->
    <string name="search_sources">Itens pesquisáveis</string>
    <string name="search_sources_summary">Escolha o que pesquisar no telefone</string>

    <!-- Title and summary used for the option to clear search shortcuts from the system
         search history -->
    <string name="clear_shortcuts">Excluir atalhos</string>
    <string name="clear_shortcuts_summary">Excluir atalhos para sugestões de pesquisa recentes</string>

    <!--[CHAR LIMIT=200] Title used in the dialog shown after choosing the 'clear search shortcuts' setting. -->
    <string name="clear_shortcuts_prompt">"Você deseja limpar todos os atalhos para sugestões de pesquisa recentes?\n\nPara limpar seu histórico de navegação, acesse as configurações do Navegador."</string>
    <!--[CHAR LIMIT=30] Text used on the button to clear shortcuts in the dialog shown after choosing the 'clear search shortcuts' setting. -->
    <string name="clear_shortcuts_agree">Excluir atalhos</string>
    <!--[CHAR LIMIT=30] Text used on the button to cancel in the dialog shown after choosing the 'clear search shortcuts' setting. -->
    <string name="clear_shortcuts_disagree">"Não excluir atalhos"</string>

    <!-- Help menu item. Opens a web page with information about how to use Quick Search Box.
         [CHAR LIMIT=20] -->
    <string name="menu_help">Ajuda</string>

    <!-- Search source label for the Google search source. -->
    <string name="google_search_label">Google</string>

    <!-- The search activity label. -->
    <string name="google_app_label">Pesquisa Google</string>

    <!-- This is the hint text shown in the search widget, before text is entered. -->
    <!-- This translation MUST MATCH the string "search_hint" which is found in
         Home/res/values/strings.xml -->
    <string name="google_search_hint">Pesquisar</string>

    <!-- Search settings description for the Google search source. [CHAR LIMIT=50] -->
    <string name="google_search_description">Sugestão de pesquisa</string>

    <!-- Title for Voice Search hints bubble -->
    <string name="voice_search_hint_title">Tente dizer:</string>
    <!-- Starting quotation marks of the voice search hint -->
    <string name="voice_search_hint_quotation_start">\"</string>
    <!-- Ending quotation marks of the voice search hint -->
    <string name="voice_search_hint_quotation_end">\"</string>

    <string name="search_in_web">Pesquisar na web</string>
    <string name="search_in_app_store">Pesquisar no market</string>
</resources>
